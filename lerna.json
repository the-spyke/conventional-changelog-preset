{
	"changelogPreset": ".",
	"command": {
		"publish": {
			"message": "chore(release): %s"
		},
		"version": {
			"ignoreChanges": [
				"**/__snapshots__/",
				"**/*.test.*.?(c|m)js",
				"**/*.test.?(c|m)js",
				"*.config.?(c|m)js",
				"*.json",
				"*.md?(x)"
			]
		}
	},
	"npmClient": "yarn",
	"packages": [
		"."
	],
	"version": "1.0.1"
}
