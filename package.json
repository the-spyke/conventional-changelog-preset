{
  "name": "@spyke/conventional-changelog-preset",
  "version": "1.0.4",
  "description": "A preset for Conventional Changelog making output similar to Babel's changelog with emojis, fancy categories and sorting.",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "commonjs",
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "echo 'Empty build step'",
    "clear": "rm -rf build dist node_modules",
    "dist": "rm -rf dist && mkdir dist && cd dist && npm pack ..",
    "lint": "eslint --format='unix' '**/*.?(c|m)js'",
    "release": "lerna version --no-git-tag-version --no-private --no-push --conventional-commits",
    "test": "jest"
  },
  "dependencies": {
    "compare-func": "^2.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@spyke/eslint-config": "^1.1.0",
    "conventional-changelog-core": "^4.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "git-dummy-commit": "^1.3.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "shelljs": "^0.8.4",
    "tmp": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/spyke-conventional-changelog-preset"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "templates/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "babel",
    "changelog",
    "conventional-changelog",
    "preset"
  ]
}
